{
  "name": "@typesafe-intl/react-intl",
  "version": "0.0.5",
  "description": "A wrapper around the popular react-intl library integrating `@typesafe-intl/core` for automatic typesafe interpolation values",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/typesafe-intl-react-intl.umd.js",
  "module": "./dist/typesafe-intl-react-intl.js",
  "types": "./dist/typesafe-intl-react-intl.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/typesafe-intl-react-intl.js",
      "require": "./dist/typesafe-intl-react-intl.umd.js"
    }
  },
  "author": "jarvispact <jarvispact@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist && vite build && tsc -p ./tsconfig-build.json",
    "preview": "vite preview",
    "publish-to-npm": "npm publish",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "peerDependencies": {
    "react-intl": "^6.0.1",
    "@typesafe-intl/core": "^0.0.5"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "react-intl": "6.5.5",
    "@typesafe-intl/core": "^0.0.5"
  }
}
