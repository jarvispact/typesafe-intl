{
  "name": "@typesafe-intl/core",
  "version": "0.0.6",
  "description": "A ICU message format parser written in Typescript's typesystem for interpolation values that are inferred automatically.",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/typesafe-intl-core.umd.js",
  "module": "./dist/typesafe-intl-core.js",
  "types": "./dist/typesafe-intl-core.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/typesafe-intl-core.js",
      "require": "./dist/typesafe-intl-core.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist && vite build && tsc",
    "preview": "vite preview",
    "publish-to-npm": "npm publish",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "typesafe",
    "intl",
    "i18n",
    "react",
    "formatjs",
    "icu",
    "translations"
  ],
  "author": "jarvispact <jarvispact@gmail.com>",
  "license": "MIT"
}
